{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dotnet",
			"task": "clean",
			"group": "clean",
			"problemMatcher": [],
			"label": "dotnet: clean"
		},
		{
			"taskName": "build Debug",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/PlayerStatsSync.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"taskName": "build Release",
			"command": "dotnet",
			"type": "process",
			"group": "build",
			"args": [
				"build",
				"${workspaceFolder}/PlayerStatsSync.csproj",
				"-c",
				"Release"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"type": "dotnet",
			"task": "build",
			"group": "build",
			"problemMatcher": [],
			"label": "dotnet: build"
		}
	]
}